{
  "name": "mnemosyne",
  "version": "1.0.0",
  "description": "Professional AI agent platform for Obsidian with local AI, advanced RAG, and specialized knowledge assistants",
  "repository": {
    "type": "git",
    "url": "https://github.com/ddunnock/mnemosyne.git"
  },
  "main": "main.js",
  "scripts": {
    "dev": "npm run prepare:dist && npm run build:tailwind && npm run copy:manifest && node esbuild.config.mjs",
    "dev:watch": "npm run prepare:dist && npm run copy:manifest && npx tailwindcss -i src/styles.input.css -o dist/styles.css --watch & node esbuild.config.mjs",
    "preserve:data": "./preserve-data.sh cleanup",
    "backup:data": "./preserve-data.sh backup",
    "restore:data": "./preserve-data.sh restore",
    "prepare:dist": "mkdir -p dist",
    "build": "npm run backup:data && npm run prepare:dist && npm run build:tailwind && npm run build:svelte && tsc --noEmit && node esbuild.config.mjs production && npm run copy:manifest && npm run copy:assets && npm run copy:wasm && npm run restore:data",
    "copy:manifest": "cp src/manifest.json dist/manifest.json",
    "copy:assets": "cp -r assets dist/",
    "copy:wasm": "cp node_modules/sql.js/dist/sql-wasm.wasm dist/",
    "build:tailwind": "npx tailwindcss -i src/styles.input.css -o dist/styles.css --minify",
    "build:svelte": "vite build",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "version": "npm run build && git add -A src"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "ai",
    "agents",
    "rag",
    "llm",
    "local-ai",
    "ollama",
    "professional",
    "knowledge-management",
    "react",
    "tailwind"
  ],
  "author": "David Dunnock",
  "license": "MIT",
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/typography": "^0.5.19",
    "@types/jest": "^29.5.5",
    "@types/node": "^16.11.6",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "autoprefixer": "^10.4.21",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "lint-staged": "^13.2.3",
    "obsidian": "latest",
    "postcss": "^8.5.6",
    "prettier": "^2.8.8",
    "svelte": "^5.41.0",
    "tailwindcss": "^3.4.18",
    "ts-jest": "^29.1.1",
    "tslib": "2.4.0",
    "typescript": "^5.3.0",
    "vite": "^7.1.10"
  },
  "dependencies": {
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.38.6",
    "@xenova/transformers": "^2.17.2",
    "crypto-js": "^4.2.0",
    "minimatch": "^10.0.3",
    "openai": "^6.4.0",
    "pg": "^8.13.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sql.js": "^1.13.0",
    "sqlite-vss": "^0.1.2"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{md,css}": [
      "prettier --write"
    ]
  }
}
